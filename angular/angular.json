{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "app-demo-client": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/app-demo-client",
      "sourceRoot": "projects/app-demo-client/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "index": "projects/app-demo-client/src/index.html",
            "browser": "projects/app-demo-client/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/app-demo-client/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/app-demo-client/public"
              }
            ],
            "styles": [
              "@angular/material/prebuilt-themes/azure-blue.css",
              "projects/app-demo-client/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kB",
                  "maximumError": "4kB"
                }
              ],
              "outputHashing": "all"
            },
            "mock": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "projects/app-demo-client/src/environments/environment.ts",
                  "with": "projects/app-demo-client/src/environments/environment.mock.ts"
                }
              ]
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "outputPath": "../node/core/dist/frontend",
              "fileReplacements": [
                {
                  "replace": "projects/app-demo-client/src/environments/environment.ts",
                  "with": "projects/app-demo-client/src/environments/environment.development.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "app-demo-client:build:production"
            },
            "mock": {
              "buildTarget": "app-demo-client:build:mock"
            },
            "development": {
              "buildTarget": "app-demo-client:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/app-demo-client/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/app-demo-client/public"
              }
            ],
            "styles": [
              "@angular/material/prebuilt-themes/azure-blue.css",
              "projects/app-demo-client/src/styles.scss",
              "projects/app-demo-client/src/themes.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "safecility-auth": {
      "projectType": "library",
      "root": "projects/safecility-auth",
      "sourceRoot": "projects/safecility-auth/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/safecility-auth/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/safecility-auth/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/safecility-auth/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/safecility-auth/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  }
}
